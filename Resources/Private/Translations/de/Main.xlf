                <target>Host</target>
            </trans-unit>
            <trans-unit id="label.protocol" xml:space="preserve">
                <source>Protocol</source>
                <target>Protokoll</target>
            </trans-unit>
            <trans-unit id="label.ip" xml:space="preserve">
                <source>IP Address</source>
